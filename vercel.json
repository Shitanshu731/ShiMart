{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "functions": {
      "api/auth/register": { "maxDuration": 30 },
      "api/auth/login": { "maxDuration": 30 }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ],
    "env": {
      "NEXT_PUBLIC_BACKEND_ENDPOINT": "https://mart-kappa.vercel.app"
    }
  }
  